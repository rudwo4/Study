import java.util.*;

public class Main{
  public static void main(String[] args){        
    Scanner input = new Scanner(System.in);
    int a = input.nextInt();
    int []Fibo = new int[a+1];
    for(int i=0;i<Fibo.length;i++){
       if(i==0){
         Fibo[i]=0;
       }else if(i==1){
         Fibo[i]=1;
       }else{
         Fibo[i]=Fibo[i-1] + Fibo[i-2];
       }
    }
    System.out.println(Fibo[a]);
  }
}
